<Window x:Class="DriveMyself.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DriveMyself"
        mc:Ignorable="d"
        Title="Доеду сам" Height="720" Width="1280"
        Loaded="Window_Loaded">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="50"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Border Background="#FFFF9C1A">
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                <TextBlock FontSize="32" Margin="5"  VerticalAlignment="Center" Foreground="White">ДОЕДУ САМ</TextBlock>
                <Image Source="Pages/Images/service_logo.png" HorizontalAlignment="Right" Margin="5" />
            </StackPanel>
        </Border>
        <Frame Name="mainFrame" Grid.Row="1" NavigationUIVisibility="Hidden" />
    </Grid>
</Window>
